<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Not_Editing_Allow_While_In_Progress</fullName>
    <active>false</active>
    <errorConditionFormula>ISPICKVAL(PRIORVALUE( Status__c ), &quot;In Progress&quot;)</errorConditionFormula>
    <errorMessage>A project can not be update while is in &apos;In Progress&apos;</errorMessage>
</ValidationRule>
